  "license": "MIT",
  "bugs": {
    "url": "https://github.com/gabrielmoreira/riot-router/issues"
  },
  "homepage": "https://github.com/gabrielmoreira/riot-router#readme",
  "files": [
    "lib/router.js",
    "lib/router.min.js",
    "lib/router.js.map",
    "lib/router.js.min.map",
    "LICENSE"
  ]
